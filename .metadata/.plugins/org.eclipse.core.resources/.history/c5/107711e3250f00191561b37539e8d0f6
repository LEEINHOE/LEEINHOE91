import java.util.Scanner;

public class Q_9 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		
		for(int i=2; i<10; i++) {
			System.out.println('\n');
		for(int j=9; j<10; j--)	{
			System.out.printf("       	" + "%d x %d = %d \n", i, j, i*j);
			
			
		}
	}
	
		}
	}


